---

interface Props {
	path: string;
	target: string;
	href: string;
}

const {href, target } = Astro.props;

// Check if it's an internal link (starts with / but not //)
const isInternal = href.startsWith('/') && !href.startsWith('//');

---

<a class={''} href={href} target={target} data-swup-preload={isInternal}><slot/></a>


